Algoritmo "exer-n"
// leia três valores  numéricos
// apresentar como resultado o valor do quadrado da soma dos tres valores lidos
Var

a, b, c, r : real

Inicio

escreva("Insira o valor de A:")
leia(a)

escreva("Insira o valor de B:")
leia(b)

escreva("Insira o valor de C:")
leia(c)

r <- (a + b + c)
r <- exp(r,2)

escreva(r)

Fimalgoritmo